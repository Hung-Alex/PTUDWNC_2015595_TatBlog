<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width ,initial-scale=1.0" />
    <title>@ViewData["Title"] -Tip & Trips Block </title>
    <!-- cái này lỗi em ko biết sửa sao do nó ko ăn cái bootrap của  lib ,file libman đã có thư viện mà vẫn ko dc nên em dùng cdn trực tiếp của bootrap    -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <link styel="stylesheet" href="~/lib/bootstrap/dist/css//bootstrap.min.css"/>
    <link style="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css"/>
    <link style="stylesheet" href="~/css/main.css" asp-append-version=true/>   
    @await RenderSectionAsync("styles",false)
</head>
<body>
    <!--Thanh tiêu đề ,logo,menu-->
    <partial name="_Header"/>

    <div class="container-fluid">
        <div class="row">

            <div class="col-9">
                <!--phần nội dung chính-->
                @RenderBody()
            </div>

            <div class="col-3 border-start">
               <partial name="_SideBar"/>
            </div>

        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container-fluid text-center">
            2023 Tip & Tricks Blog
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>

    @await RenderSectionAsync("scripts",false)
</body>
</html>
